<html>
    <head>
        <title><%= @getDocumentTitle() %></title>
        <%- @getBlock("meta").toHTML() %>
        <%- @getBlock("styles").add(["/styles/main.css"]).toHTML() %>
        <!--<meta charset="utf-8" />-->
        <!--<meta name="viewport" content="width=device-width, initial-scale=1" />-->
        <!--[if lte IE 8]><script src="vendor/ie/html5shiv.js"></script><![endif]-->
        <!--[if lte IE 9]><link rel="stylesheet" href="styles/ie9.css" /><![endif]-->
        <!--[if lte IE 8]><link rel="stylesheet" href="styles/ie8.css" /><![endif]-->
    </head>
    <body class="<%= @getDocumentCssClass() %>">
		<%- @content %>
		<%- @getBlock("scripts")
			.add([
				"/vendor/jquery.min.js",
				"/vendor/jquery.scrolly.min.js",
				"/vendor/jquery.dropotron.min.js",
				"/vendor/jquery.scrollex.min.js",
				"/vendor/skel.min.js",
				"/scripts/util.js",
				"/scripts/main.js"
			]).toHTML()
		%>
    </body>
</html>
